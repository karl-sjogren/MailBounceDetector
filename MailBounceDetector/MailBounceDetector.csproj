<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>net452;netstandard2.0</TargetFrameworks>
    <Description>Detects whether a MailKit email Message is a bounce message</Description>
    <Version>1.0.4</Version>
    <Company>ruilopes.com</Company>
    <Copyright>Copyright ruilopes.com</Copyright>
    <PackageLicenseUrl>https://opensource.org/licenses/MIT</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/rgl/MailBounceDetector</PackageProjectUrl>
    <PackageTags>MailKit Bounce</PackageTags>
    <AllowedOutputExtensionsInPackageBuildOutputFolder>$(AllowedOutputExtensionsInPackageBuildOutputFolder);.pdb</AllowedOutputExtensionsInPackageBuildOutputFolder>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="MimeKit" Version="2.6.0" />
  </ItemGroup>
</Project>